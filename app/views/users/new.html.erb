<%= form_with model: @user, url: users_path, local: true do |form| %>

	<p>
		<%= form.label :username %><br>
		<%= form.text_field :username %>
	</p>
	<p>
		<%= form.label :email %><br>
		<%= form.text_field :email %>
	</p>

	<p>
		<%= form.label :password %><br>
		<%= form.text_area :password %>
	</p>

	<p>
		<%= form.submit "Sign Up"%>
	</p>
<% end %>
<%= link_to 'Back', users_path %>